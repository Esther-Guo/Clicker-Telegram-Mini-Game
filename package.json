{
  "name": "clicker-telegram-mini-game",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "client:build": "tsc && vite build",
    "client:preview": "vite preview",
    "server:dev": "tsx src/backend/server.ts",
    "server:build": "tsc -p tsconfig.server.json",
    "server:start": "node dist/backend/server.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "npm run server:start"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.10",
    "@types/express": "^4.17.x",
    "@types/node": "^18.x.x",
    "cors": "^2.8.x",
    "dotenv": "^16.4.7",
    "express": "^4.18.x",
    "react": "^18.x.x",
    "react-dom": "^18.x.x",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.x.x"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/react": "^18.x.x",
    "@types/react-dom": "^18.x.x",
    "@vitejs/plugin-react": "^4.x.x",
    "concurrently": "^8.2.2",
    "tsx": "^4.19.2",
    "vite": "^4.x.x"
  }
}
